<rdf:Description rdf:about="https://spec.industrialontologies.org/ontology/lca/core#LifeCycleInventoryAnalysis">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
	<subClassOf xmlns="http://www.w3.org/2000/01/rdf-schema#" rdf:resource="https://spec.industrialontologies.org/ontology/core/Core/PlannedProcess"/>
	<title xmlns="http://purl.org/dc/terms/">life cycle inventory analysis</title>
	<naturalLanguageDefinition xmlns="https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/">phase of life cycle assessment involving the compilation and quantifica-tion of inputs and outputs for a product throughout its life cycle</naturalLanguageDefinition>
</rdf:Description>

<rdf:Description rdf:about="https://spec.industrialontologies.org/ontology/lca/core#LifeCycleAssessmentProcess">
	<rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
